{
  "name": "bitmovin-javascript",
  "version": "1.10.0",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --color ./bitmovin",
    "lint-fix": "./node_modules/.bin/eslint --color --fix ./bitmovin",
    "test": "node scripts/testRunner.js tests/",
    "test_it": "node scripts/test.js tests_it/",
    "coverage": "node scripts/testRunner.js tests/ --coverage",
    "it-coverage": "rm -rf ./coverage && npm run analyze-coverage && npm run check-coverage",
    "it": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --require babel-polyfill --recursive tests_it --timeout 30000",
    "debug": "./node_modules/webpack/bin/webpack.js --config=webpack.config.debug.js",
    "release": "./node_modules/webpack/bin/webpack.js --config=webpack.config.release.js",
    "publish-release": "cd ./build/release && npm publish",
    "format": "./node_modules/.bin/prettier --config ./.prettierrc --write \"./{bitmovin,examples}/**/*.{js,scss}\"",
    "format-check": "./node_modules/.bin/prettier --config ./.prettierrc --list-different \"./{bitmovin,examples}/**/*.{js,scss}\""
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chokidar": "^1.6.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.13.1",
    "isparta": "^4.0.0",
    "istanbul": "v1.1.0-alpha.1",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "nodelint": "^0.6.2",
    "prettier": "^1.12.1",
    "regenerator-runtime": "^0.10.3",
    "url-join": "^1.1.0",
    "webpack": "^1.14.0",
    "write-json-webpack-plugin": "^1.0.0",
    "xmlhttprequest": "^1.8.0"
  },
  "dependencies": {
    "bitmovin-javascript": "^1.10.0",
    "bluebird": "^3.4.7",
    "codecov": "^2.2.0",
    "moment": "^2.17.1",
    "node-fetch": "^1.6.3"
  }
}
